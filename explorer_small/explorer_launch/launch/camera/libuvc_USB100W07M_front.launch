<?xml version="1.0"?>
<launch>
  <group ns="camera">
    <node pkg="libuvc_camera" type="camera_node" name="USB100W07M_front">
      <!-- Parameters used to find the camera -->
      <param name="vendor" value="0x05a3"/>
      <param name="product" value="0x9601"/>
      <param name="serial" value="3"/>
      <!-- If the above parameters aren't unique, choose the first match: -->
      <param name="index" value="0"/>

      <!-- Image size and type -->
      <param name="width" value="640"/>
      <param name="height" value="480"/>
      <!-- choose whichever uncompressed format the camera supports: -->
      <param name="video_mode" value="yuyv"/> <!-- or yuyv/nv12/jpeg -->
      <param name="frame_rate" value="30"/>

      <!--<param name="timestamp_method" value="start"/>  start of frame -->
      <param name="frame_id" value="camera_front"/>
      <param name="camera_info_url" value="file:///$(find explorer_launch)/launch/camera/yaml/USB100W07M.yaml" />

      <!-- use aperture_priority auto exposure -->
      <param name="auto_exposure" value="3"/> 
      <param name="auto_white_balance" value="true"/>

      <param name="auto_focus" value="true" />

      <remap from="/camera/image_raw" to="/camera_front/image_raw"/>
    </node>
	<!--<node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
    	<remap from="image" to="/camera/rgb/image_raw"/>
    	<param name="autosize" value="true" />
  	</node>-->
  </group>
</launch>
